{
  "private": true,
  "scripts": {
    "dev": "bun run vite",
    "build": "bun run tsc && bun run vite build",
    "preview": "bun run vite preview",
    "prettier": "bun run prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@ant-design/pro-layout": "^7.5.1",
    "@react-three/drei": "^9.17.3",
    "@react-three/fiber": "^8.2.1",
    "antd": "^5.1.2",
    "less": "^4.1.3",
    "less-loader": "^11.0.0",
    "nanoid": "^4.0.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-spring": "^9.5.2",
    "three": "^0.148.0",
    "video.js": "^7.20.1"
  },
  "devDependencies": {
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "@types/video.js": "^7.3.40",
    "@vitejs/plugin-react": "^3.0.0",
    "lint-staged": "^13.0.0",
    "prettier": "^2.2.0",
    "typescript": "^4.1.2",
    "vite": "^4.0.4",
    "yorkie": "^2.0.0"
  }
}
